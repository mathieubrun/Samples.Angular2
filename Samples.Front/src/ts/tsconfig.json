{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "target": "es5",
    "module": "commonjs",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "../../wwwroot/app",
    "baseUrl": ".",
    "diagnostics": true,
    "pretty": true
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
